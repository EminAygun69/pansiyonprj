        <a href="/giris">Giriş Yap</a>
        <a href="/profil">Profil</a>
    </nav>

    <br><br><br>
    <form action="/auth/register" method="post">
        <label for="usernamereg">Username</label>
        <input type="text" name="usernamereg" required>

        <label for="emailreg">Email</label>
        <input type="email" name="emailreg" required>

        <label for="passwordreg">Password</label>
        <input type="password" name="passwordreg" required>

        <label for="phonenumberreg">Phone Number</label>
        <input type="tel" value="90" id="phonenumberreg" name="phonenumberreg" required>
        <input type="submit">
    </form>
    <br><br><br>
    
</body>
</html>